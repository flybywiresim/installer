{
  "name": "fbw-installer",
  "productName": "FlyByWire Installer",
  "version": "2.0.0-dev17",
  "description": "Desktop application to install and customize FlyByWire mods",
  "main": "webpack/main/main.js",
  "scripts": {
    "development": "set NODE_ENV=development",
    "production": "set NODE_ENV=production",
    "compile:main": "webpack --config webpack.main.config.js",
    "compile:renderer": "webpack --config webpack.renderer.config.js",
    "compileProd:main": "webpack --config webpack.main.config.js",
    "compileProd:renderer": "webpack --config webpack.renderer.config.js",
    "compile": "npm run development && npm run compile:main && npm run compile:renderer",
    "compileProd": "npm run production && npm run compileProd:main && npm run compileProd:renderer",
    "start": "npm run compile && electron webpack/main/main.js",
    "package": "npm run compileProd && electron-builder --win --publish=never",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "FlyByWire Simulations",
  "license": "GPL-3.0",
  "build": {
    "extraResources": [
      "./extraResources/**"
    ],
    "appId": "com.flybywiresim.installer",
    "nsis": {
      "include": "scripts/installer.nsh",
      "artifactName": "FlyByWire_Installer_Setup.${ext}"
    },
    "win": {
      "target": "nsis",
      "icon": "src/main/icons/icon.ico",
      "files": [
        "webpack/**/*"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@flybywiresim/api-client": "~0.5.0",
    "@flybywiresim/tailwind-config": "~0.3.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/postcss7-compat": "~2.0.3",
    "@types/adm-zip": "^0.4.33",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.1.7",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@webpack-cli/serve": "^1.3.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^4.3.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.11.8",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.22.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^4.3.8",
    "inspectpack": "^4.7.1",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.4",
    "node-loader": "^1.0.1",
    "postcss": "~7.0.35",
    "postcss-loader": "~4.2.0",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.2",
    "react-svg-loader": "^3.0.3",
    "style-loader": "^1.3.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.0.3",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "type-fest": "^2.0.0",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.29.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@flybywiresim/fragmenter": "^0.5.2",
    "@flybywiresim/react-components": "^0.2.5",
    "antd": "^4.12.2",
    "config-ini-parser": "~1.5.9",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.9",
    "fs-extra": "^8.1.0",
    "immer": "^8.0.1",
    "lodash": "^4.17.20",
    "marked": "^2.0.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-detect-offline": "^2.4.1",
    "react-dom": "^17.0.1",
    "react-html-parser": "^2.0.2",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "simplebar-react": "^2.3.0",
    "styled-components": "^5.2.1",
    "tabler-icons-react": "~1.33.0",
    "walkdir": "^0.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  }
}
