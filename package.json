{
  "name": "fbw-installer",
  "productName": "FlyByWire Installer",
  "version": "3.5.0-dev.1",
  "description": "Desktop application to install and customize FlyByWire addons",
  "author": {
    "name": "FlyByWire Simulations",
    "email": "contact@flybywiresim.com",
    "url": "https://flybywiresim.com"
  },
  "license": "GPL-3.0",
  "configUrls": {
    "production": "https://flybywirecdn.com/installer/config/production.json",
    "confidentialBaseUrl": "https://flybywirecdn.com/installer/config/confidential"
  },
  "main": "out/main/index.js",
  "scripts": {
    "generate-licenses": "node scripts/licenses.js",
    "start": "electron-vite",
    "dev": "electron-vite dev",
    "build": "npm run generate-licenses && electron-vite build",
    "package": "npm run build && electron-builder --publish=never",
    "package:all": "npm run build && electron-builder --win --linux --publish=never && electron-builder --config electron-builder-snap.yml --linux snap --publish=never && DEBUG=* electron-builder --config electron-builder-flatpak.yml --linux flatpak --publish=never",
    "package:win": "npm run build && electron-builder --win --publish=never",
    "package:linux": "npm run build && electron-builder --linux --publish=never && electron-builder --config electron-builder-snap.yml --linux snap --publish=never && electron-builder --config electron-builder-flatpak.yml --linux flatpak --publish=never",
    "package:appimage": "npm run build && electron-builder --linux AppImage --publish=never",
    "package:deb": "npm run build && electron-builder --linux deb --publish=never",
    "package:rpm": "npm run build && electron-builder --linux rpm --publish=never",
    "package:snap": "npm run build && electron-builder --config electron-builder-snap.yml --linux snap --publish=never",
    "package:flatpak": "npm run build && electron-builder --config electron-builder-flatpak.yml --linux flatpak --publish=never",

    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx ."
  },
  "keywords": [],
  "build": {
    "extraResources": [
      "./extraResources/**"
    ],
    "appId": "com.flybywiresim.installer",
    "nsis": {
      "include": "scripts/installer.nsh",
      "artifactName": "FlyByWire_Installer_Setup.${ext}"
    },
    "win": {
      "target": "nsis",
      "icon": "src/main/icons/icon.ico",
      "files": [
        "out/**/*"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "icon": "build/icons/fbw-installer",
      "executableName": "fbw-installer",
      "executableArgs": [
        "--no-sandbox"
      ],
      "synopsis": "Install and customize FlyByWire addons",
      "description": "Desktop application to install and customize FlyByWire addons",
      "desktop": {
        "entry": {
          "Name": "FlyByWire Installer",
          "Comment": "Desktop application to install and customize FlyByWire addons",
          "Type": "Application",
          "Categories": "Utility;",
          "StartupWMClass": "FlyByWire Installer",
          "Icon": "fbw-installer"
        }
      },
      "files": [
        "out/**/*"
      ],
      "category": "Utility"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@csstools/postcss-sass": "^5.1.1",
    "@electron/remote": "^2.1.3",
    "@flybywiresim/api-client": "^0.15.0",
    "@flybywiresim/tailwind-config": "^0.3.1",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/postcss7-compat": "~2.0.3",
    "@types/adm-zip": "^0.4.34",
    "@types/dateformat": "^3.0.1",
    "@types/electron-devtools-installer": "^2.2.5",
    "@types/lodash": "^4.17.21",
    "@types/react": "^17.0.90",
    "@types/react-dom": "^17.0.26",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.36",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@webpack-cli/serve": "^1.7.0",
    "babel-loader": "^8.4.1",
    "concurrently": "^6.5.1",
    "css-loader": "^4.3.0",
    "electron": "^25.9.8",
    "electron-builder": "^26.0.20",
    "electron-devtools-installer": "^3.2.1",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "html-webpack-plugin": "^5.6.5",
    "inspectpack": "^4.7.1",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.5.4",
    "node-loader": "^1.0.3",
    "postcss": "^8.5.6",
    "postcss-import": "^14.1.0",
    "postcss-loader": "~4.2.0",
    "precss": "^4.0.0",
    "react-svg-loader": "^3.0.3",
    "redux-devtools-extension": "^2.13.9",
    "style-loader": "^1.3.0",
    "tailwindcss": "^3.4.19",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.1.1",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "vite": "^5.4.21",
    "vite-plugin-commonjs-externals": "^0.1.4",
    "vite-plugin-electron-renderer": "^0.14.6",
    "webpack": "^5.104.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "@flybywiresim/fragmenter": "^0.8.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@sentry/cli": "^2.58.4",
    "@sentry/electron": "^4.24.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-virtual": "^3.13.13",
    "@types/js-yaml": "^4.0.9",
    "@types/winreg": "^1.2.36",
    "@types/ws": "^8.18.1",
    "check-disk-space": "^3.4.0",
    "clsx": "^2.1.1",
    "config-ini-parser": "~1.5.9",
    "dateformat": "^5.0.3",
    "electron-store": "^8.2.0",
    "electron-updater": "^4.6.5",
    "fs-extra": "^8.1.0",
    "immer": "^10.2.0",
    "js-yaml": "^4.1.1",
    "js-yaml-loader": "^1.2.2",
    "marked": "^2.1.3",
    "nanoid": "^4.0.2",
    "pdf-to-printer": "5.3.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-bootstrap-icons": "^1.11.6",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.1",
    "react-intersection-observer": "^8.34.0",
    "react-markdown": "^8.0.7",
    "react-redux": "^7.2.9",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.3.4",
    "react-windows-controls": "^1.1.1",
    "redux": "^4.2.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^2.0.0",
    "semver": "7.3.5",
    "simplebar-react": "^3.3.2",
    "styled-components": "^5.3.11",
    "tabler-icons-react": "~1.33.0",
    "tailwind-merge": "^2.6.0",
    "walkdir": "^0.4.1",
    "winreg": "^1.2.5",
    "ws": "^8.18.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "utf-8-validate": "^5.0.10"
  }
}
